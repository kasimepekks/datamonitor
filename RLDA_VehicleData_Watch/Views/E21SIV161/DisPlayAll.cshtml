
@{
    ViewData["Title"] = "E21SIV161_INFO";
    Layout = "~/Views/Shared/_Layout_DisplayAll.cshtml";
}
@section Style{

    <meta name="viewport" content="width=device-width" />

    <script src="~/lib/echarts/echarts.min.js"></script>
    <script type="text/javascript" src="//api.map.baidu.com/api?v=2.0&ak=FFvnSXHMHaC0CQgTguKY17zkEwwfqjsP"></script>
    <script src="~/MYJS/GPSConverttoBaidu.js"></script>

    <script src="~/MYJS/LuShu_min.js"></script>
    <script src="~/MYJS/E21SIV161_DisplayAll.js"></script>
    <style type="text/css">
        body {
            text-align: center
        }
    </style>
}
<div class="layui-fluid">

    <fieldset class="layui-elem-field">
        <legend>E21SIV161 行驶里程</legend>
        <div class="layui-field-box">
            <span id="distance" class="layui-bg-black" style="font-size:28px"></span>公里
        </div>
    </fieldset>

    <div class="layui-row">

        <div class="layui-col-md6">
            <div id="speedchart"></div>

        </div>

        <div class="layui-col-md6">
            <div id="brakechart"></div>

        </div>
    </div>
    <div class="layui-row">

        <div id="allmap" style="width: 100%;height:800px"></div>

    </div>
</div>
